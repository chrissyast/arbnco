<% if association.is_a? ActiveRecord::Associations::CollectionProxy %>
  <% association.each do |a| %>
    <%= render partial: "association", locals: { association: a } %>
  <% end %>
<% else %>
  <h5>
    <%= association.name %>
  </h5>
  <% association.attributes.except("id", "upload_id", "created_at", "updated_at", "name").each do |k, v| %>
    <% if v and v.strip != "" %>
      <li>
        <b><%= k %></b>: <%= v %>
      </li>
    <% end %>
  <% end %>
<% end %>