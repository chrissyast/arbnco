<li>
  <details>
    <summary>
      <%= object.name %>
    </summary>
    <ul>
      <% object.attributes.except("id", "inp_file_id", "created_at", "updated_at", "name").each do |k, v| %>
        <% if v and v.strip != "" %>
          <li>
            <b><%= k == "x_type" ? "type" : k %></b>: <%= v %>
          </li>
        <% end %>
      <% end %>
    </ul>
  </details>
</li>